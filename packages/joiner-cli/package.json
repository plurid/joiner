        "build.cli.production": "rollup -c ./scripts/rollup.config.ts && pnpm build.clean",
        "build.development": "pnpm build.dashboard.development && pnpm build.cli.development",
        "build": "pnpm build.dashboard.production && pnpm build.cli.production",
